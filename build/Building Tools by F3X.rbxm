&#9;&#9;&#9;&#9;if self.Temporary.Connections.HandleReleaseListener then
&#9;&#9;&#9;&#9;&#9;self.Temporary.Connections.HandleReleaseListener:disconnect();
&#9;&#9;&#9;&#9;&#9;self.Temporary.Connections.HandleReleaseListener = nil;
&#9;&#9;&#9;&#9;end;

