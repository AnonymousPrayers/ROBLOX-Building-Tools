&#9;&#9;&#9;show_add = false;
&#9;&#9;&#9;show_remove = true;

&#9;&#9;-- If no item has a mesh
&#9;&#9;elseif #meshes == 0 then
&#9;&#9;&#9;show_add = true;
&#9;&#9;&#9;show_remove = false;

&#9;&#9;-- If some items have a mesh
&#9;&#9;else
