&#9;&#9;&#9;updater_on = false;
&#9;&#9;end;

&#9;&#9;while wait( 0.1 ) and updater_on do

&#9;&#9;&#9;-- Make sure the tool&apos;s equipped
&#9;&#9;&#9;if CurrentTool == Tools.Move then

&#9;&#9;&#9;&#9;-- Update the GUI if it&apos;s visible
